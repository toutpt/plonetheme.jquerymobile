      permission="zope2.View"
      class="plonetheme.jquerymobile.browser.viewlets.manager.WeightedOrderedViewletManager"
      />
  <browser:viewletManager
      name="plonetheme.jquerymobile.editbar"
      provides="plonetheme.jquerymobile.browser.viewlets.manager.IEditBar"
      permission="zope2.View"
      class="plonetheme.jquerymobile.browser.viewlets.manager.WeightedOrderedViewletManager"
      />
  <browser:viewletManager
      name="plonetheme.jquerymobile.footer"
      provides="plonetheme.jquerymobile.browser.viewlets.manager.IFooter"
      permission="zope2.View"
      class="plonetheme.jquerymobile.browser.viewlets.manager.WeightedOrderedViewletManager"
      />
  <browser:viewletManager
      name="plonetheme.jquerymobile.belowcontent"
      provides="plonetheme.jquerymobile.browser.viewlets.manager.IBelowContent"
      permission="zope2.View"
      class="plonetheme.jquerymobile.browser.viewlets.manager.WeightedOrderedViewletManager"
      />

</configure>